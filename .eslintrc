{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": [
      2,
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "linebreak-style": 0
  },
  "parserOptions": {
    "ecmaVersion": "latest"
  }
}